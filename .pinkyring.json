{
  "templateName": "python-flask-template",
  "pinkyringVersion": "1.0.0",
  "removableOptions": [
    {
      "label": "DOCKER",
      "globPatterns": [".dockerignore", "Dockerfile"],
      "contentPattern": ".pinkyring=DOCKER"
    },
    {
      "label": "MONGO",
      "globPatterns": [
        "app/api/document.py",
        "app/infrastructure/document_db",
        "app/models/document.py"
      ],
      "contentPattern": ".pinkyring=MONGO",
      "packageNames": ["pymongo"]
    }
  ]
}
